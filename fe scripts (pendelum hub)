local DrRayLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/AZYsGithub/DrRay-UI-Library/main/DrRay.lua"))()
local window = DrRayLibrary:Load("Pendulum Hub Remake", "Default")

local tab = DrRayLibrary.newTab("Scripts", "ImageIdHere")

local tab1 = DrRayLibrary.newTab("Settings", "ImageIdHere")

local tab2 = DrRayLibrary.newTab("Credits", "ImageIdHere")

tab.newButton("if u want use hat scripts check source of script and equip", "", function()
print("Hello!")
end)

